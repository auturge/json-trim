{
    "name": "@auturge/json-trim",
    "description": "Copies a json file, trimming out all but a select list of keys.",
    "version": "1.0.0",
    "author": "auturge",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/auturge/json-trim/issues"
    },
    "homepage": "https://github.com/auturge/json-trim",
    "repository": {
        "type": "git",
        "url": "https://github.com/auturge/json-trim"
    },
    "engines": {
        "node": ">=12.8.1"
    },
    "bin": "./bin/json-trim.js",
    "main": "./lib/JsonTrim.js",
    "devDependencies": {
        "@auturge/expect-cwd": "latest",
        "@auturge/testing": "latest",
        "chai": "^4.3.0",
        "coveralls": "^3.1.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "babel-eslint": "^10.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "mocha": "^10.1.0",
        "mocha-lcov-reporter": "^1.3.0",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "shx": "^0.3.3",
        "sinon": "^9.2.4",
        "tslib": "2.0.1"
    },
    "dependencies": {
        "@auturge/config-resolver": "^1.0.0",
        "colorette": "^1.2.2",
        "command-line-usage": "^6.1.1",
        "commander": "^7.1.0",
        "is-valid-path": "^0.1.1",
        "fastest-levenshtein": "^1.0.12"
    },
    "scripts": {
        "test": "npm-run-all ~test:clean ~test:run",
        "clean": "shx rm -rf dist build test/coverage",
        "lint": "eslint . --ext .js,.jsx",
        "package": "npm-run-all clean ~mkdir:dist ~copy",
        "prepublishOnly": "expect-cwd ./dist",
        "~copy": "npm-run-all ~copy:readme ~copy:source ~copy:license ~copy:changelog ~copy:package:json",
        "~copy:changelog": "shx cp ./CHANGELOG.md ./dist",
        "~copy:package:json": "node ./src/bin/json-trim -c ./conf/trim.config.js --env=dist",
        "~copy:readme": "shx cp ./README.md ./dist",
        "~copy:source": "shx cp -R ./src/lib ./src/bin ./src/resource ./src/index.js ./dist/",
        "~copy:license": "shx cp ./LICENSE ./dist",
        "~mkdir:dist": "shx mkdir ./dist",
        "~test:clean": "shx rm -rf coverage",
        "~test:run": "nyc mocha \"./test/**/*.spec.js\"",
        "~test:report": "nyc report --reporter=text-lcov | coveralls",
        "------------": "------------------------------------------------------------",
        "package-v": "npm-run-all clean ~mkdir:dist ~copy-verbose",
        "~copy-verbose": "npm-run-all ~copy:readme ~copy:source ~copy:license ~copy:changelog ~copy:package:json:verbose",
        "~copy:package:json:verbose": "node ./src/bin/json-trim -c ./conf/trim.config.js --env=dist --verbose",
        "js": "node ./src/bin/json-trim -c ./conf/trim.config.js",
        "override": "node ./src/bin/json-trim -c ./conf/trim.config.js -s ./package.json",
        "env": "node ./src/bin/json-trim -c ./conf/trim.config.js -s ./package.json --env=dog",
        "w": "node ./src/bin/json-trim -c ./conf/trim.config.js -s ./package.json --env production=staging --env dog -w name -w version -w description",
        "h": "node ./src/bin/json-trim -h",
        "hs": "node ./src/bin/json-trim help -s",
        "hq": "node ./src/bin/json-trim help --quiet",
        "help": "node ./src/bin/json-trim --help",
        "v": "node ./src/bin/json-trim -v",
        "trim": "node ./src/bin/json-trim",
        "ver": "node ./src/bin/json-trim -ver",
        "dev": "node ./src/bin/json-trim -c ./conf/trim.config.js -s ./package.json --env dev --env type=test -k one two three",
        "prod": "node ./src/bin/json-trim -c ./test/trim.config.prod.js"
    }
}
