{
    "temp-dir": "./test/coverage/.nyc-output",
    "report-dir": "./test/coverage",
    "all": true,
    "check-coverage": true,
    "extension": [
        ".js"
    ],
    "statements": 90,
    "lines": 90,
    "functions": 80,
    "branches": 80,
    "reporter": [
        "html",
        "lcov",
        "text"
    ],
    "include": [
        "src/**/*.js"
    ],
    "exclude": [
        "src/bin/**/*.js"
    ]
}
